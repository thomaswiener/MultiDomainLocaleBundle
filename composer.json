{
    "name": "twi/locale-bundle",
    "type": "symfony-bundle",
    "description": "Simple locale bundle for symfony2",
    "keywords": ["locale"],
    "homepage": "http://prototyp.in",
    "license": "Apache",
    "authors": [
        {
            "name": "Thomas Wiener",
            "email": "wiener.thomas@googlemail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony/framework-bundle": "~2.1"
    },
    "require-dev": {
        "symfony/framework-bundle": "<2.1.5",
        "symfony/css-selector": "<2.1.5",
        "symfony/http-kernel": "<2.1.5",
        "symfony/validator": "<2.1.5",
        "symfony/browser-kit": "<2.1.5",
        "symfony/finder": "<2.1.5",
        "symfony/form": "<2.1.5",
        "symfony/yaml": "<2.1.5",
        "symfony/process": "<2.1.5",
        "symfony/routing": "<2.1.5",
        "symfony/http-foundation": "<2.1.5",
        "symfony/class-loader": "<2.1.5",
        "symfony/twig-bundle": "<2.1.5",
        "sensio/framework-extra-bundle": "<2.1.5"
    },
    "autoload": {
        "psr-0": { "TWI\\LocaleBundle": "" }
    },
    "target-dir": "TWI/LocaleBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}